public class PrimitiveDataTypeExam {
    public static void main(String[] args) {
        
boolean isFun = false; //boolean 타입의 변수 isFun을 선언하고 초기값을 false로 설정함
        
char charValue = 'a'; // char 타입의 변수 charValue를 선언하고 단일 문자'a'를 입력함.
        
int intValue = 20;  // 정수형 int 타입의 변수 intValue를 선언하고 20을 입력함.
        
long longValue = 2147483648L;  //정수형 long 타입의 longValue를 선언하고 값을 입력함.

        출력
        System.out.println(isFun);
        System.out.println(charValue);
        System.out.println(intValue);
        System.out.println(longValue);
    }
}
